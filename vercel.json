{
  "routes": [
    {
      "src": "/[^.]+",
      "dest": "/",
      "status": 200
    },
    {
      "src": "/api/(.*)",
      "headers": {
        "Access-Control-Allow-Origin": "https://leochristophe.fr",
        "Access-Control-Allow-Methods": "POST, OPTIONS, GET",
        "Access-Control-Allow-Headers": "Content-Type"
      },
      "continue": true
    }
  ]
}